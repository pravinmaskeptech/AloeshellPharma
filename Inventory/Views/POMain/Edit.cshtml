    function CalculateAmount() {
        var quantity = $('#OrderQty').val();
        var rate = $('#Price').val();
        var tax = $('#Tax').val();
        var discount = $('#Discount').val();
        var amount = $('#Amount').val();
        var discIn = $('#discIn').val()
        var taxx = 0;
        var disc = 0;
        var amt = 0;
        if (quantity == "" || rate == "" || tax == "" || discount == "") {
        }
        else {

            var total = parseFloat(quantity) * parseFloat(rate);
            // get Amount
            $('#Amount').val(parseFloat(total).toFixed(2));
            //get discount

            if (discIn == "Rupee") {
                disc = (parseFloat(discount) * parseFloat($('#OrderQty').val())).toFixed(2);
            } else {
                disc = (parseFloat(total) * parseFloat(discount)) / 100;
            }
            $('#DiscAmount').val(parseFloat(disc).toFixed(2));

            var taxamt = ((parseFloat(total) - parseFloat(disc)) * parseFloat(tax)) / 100;
            if ($('#isIGST').val() == true)
            {
                $('#IGST').val(taxamt);
            }
            else {
                var Amt = parseFloat(taxamt) / 2;
                $('#SGST').val(Amt);
                $('#CGST').val(Amt);
            }

            $('#TotAmt').val(((parseFloat(total) - parseFloat(disc)) + parseFloat(taxamt)).toFixed(2));
        }
    }
