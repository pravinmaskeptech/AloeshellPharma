    function CalculateGridAmount(Count) {

        $("#TOTAMOUNT" + Count + "").val('');
        var rate = $("#RATE" + Count + "").val();
        var qty = $("#QUANTITY" + Count + "").val();
        var disc = $("#DISCOUNT" + Count + "").val();

        var tax = $("#TAX" + Count + "").val();
        var netAmt = $("#AMOUNT" + Count + "").val();
        var DiscIn = $("#DISCIN" + Count + "").val();


        var Tamt = parseFloat(qty) * parseFloat(rate);
        $("#AMOUNT" + Count + "").val(parseFloat(Tamt).toFixed(2));
        var taxAmount = (parseFloat(Tamt) * parseFloat(tax)) / 100;

        $("#CGSTAmt" + Count + "").val(0);
        $("#SGSTAmt" + Count + "").val(0);
        $("#IGSTAmt" + Count + "").val(0);

        if ($('#isIGST').val() == true) {
            $("#IGSTAmt" + Count + "").val(taxAmount);
        } else {
            var taxx = parseFloat(taxAmount) / 2
            $("#SGSTAmt" + Count + "").val(taxx)
            $("#CGSTAmt" + Count + "").val(taxx)
        }
        var totalAmount = parseFloat(Tamt) + parseFloat(taxAmount);

        var discamt = 0;
        if (DiscIn == "Rupee") {
            $("#Disc" + Count + "").val(parseFloat(disc).toFixed(2));
            discamt = parseFloat(disc).toFixed(2);
        } else {
            discamt = (parseFloat(totalAmount) * parseFloat(disc)) / 100
            $("#Disc" + Count + "").val(parseFloat(discamt).toFixed(2));
        }

        $("#TOTAMOUNT" + Count + "").val((parseFloat(totalAmount) - parseFloat(discamt)).toFixed(2));
        getTotalAmount();

    }
